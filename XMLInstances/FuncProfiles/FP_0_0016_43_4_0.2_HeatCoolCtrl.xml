  <sgr:dpListElement>
    <sgr:dataPoint datapointName="SupplyWaterTempStptOffs" mroVisibilityIndicator="R" rwpDatadirection="RW" unit="DEGREES_CELSIUS">
      <sgr:dataType>
        <sgr:float64/>
      </sgr:dataType>
      <sgr:dpNameList>
        <sgr:sLV1Name>SupplyWaterTempStptOffs</sgr:sLV1Name>
      </sgr:dpNameList>
      <sgr:legibleDescription>
        <sgr:textElement>
          Setzen der aktuellen Soll-Temperatur Differenz des Vorlaufs (Offset
          zum Grundwert). Min / Max Val der zugelassenen Abweichung können im SGrAttribut
          gesetzt werden
        </sgr:textElement>
        <sgr:language>de</sgr:language>
      </sgr:legibleDescription>
    </sgr:dataPoint>
    <sgr:genAttribute>
      <sgr:minVal>+5.0</sgr:minVal>
      <sgr:maxVal>-5.0</sgr:maxVal>
    </sgr:genAttribute>
