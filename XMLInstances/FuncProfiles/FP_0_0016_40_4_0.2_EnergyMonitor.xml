      <sgr:functionalProfileType>EnergyMonitor</sgr:functionalProfileType>
      <sgr:levelOfOperation>m</sgr:levelOfOperation>
      <sgr:versionNumber>
        <sgr:primaryVersionNumber>0</sgr:primaryVersionNumber>
